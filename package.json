  "scripts": {
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register  --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/src/index.js",
    "prettier": "prettier --write src/",
    "prettier:check": "prettier --check src/",
    "lint": "eslint --quiet --cache --cache-strategy content src/",
    "lint:write": "eslint src/**/*.ts --fix",
    "test": "jest --passWithNoTests --detectOpenHandles --runInBand --no-cache --forceExit"
  },
  "dependencies": {
    "convict": "^6.2.4",
    "dotenv": "^16.3.1",
